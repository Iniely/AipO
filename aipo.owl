<?xml version="1.0"?>
<rdf:RDF xmlns="https://w3id.org/aipo#"
     xml:base="https://w3id.org/aipo"
     xmlns:schema="http://schema.org/"
     xmlns:aipo1="https://w3id.org/aipo#"
     xmlns:aipo2="http://purl.org/aipo#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:aipo="https://w3id.org/aipo/"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:foaf="https://w3id.org/aipo#foaf:"
     xmlns:dc="http://purl.org/dc/elements/1.1/">
    <owl:Ontology rdf:about="https://w3id.org/aipo">
        <rdfs:label xml:lang="en">Administration and International Bussiness Pensum&apos;s Ontology (AIPO)</rdfs:label>
        <rdfs:comment xml:lang="es">Ontología que describre el Pensum de la carrera de Administración y Negocios Internacionales de la Universidad Peruana Unión</rdfs:comment>
        <rdfs:comment xml:lang="en">Ontology that describes the Pensum of the Administration and International Business career of the Universidad Peruana Unión</rdfs:comment>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.w3.org/2001/XMLSchema#duration -->

    <rdfs:Datatype rdf:about="http://www.w3.org/2001/XMLSchema#duration"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/dc/elements/1.1/creator -->

    <owl:ObjectProperty rdf:about="http://purl.org/dc/elements/1.1/creator">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/aipo#Resourse"/>
        <rdfs:range rdf:resource="https://w3id.org/aipo#Person"/>
        <rdfs:comment xml:lang="es">Responsable de elaborar el recurso. Puede ser una entidad, una persona o un servicio.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://xmlns.com/foaf/0.1/member -->

    <owl:ObjectProperty rdf:about="http://xmlns.com/foaf/0.1/member">
        <rdfs:isDefinedBy>http://xmlns.com/foaf/0.1/member</rdfs:isDefinedBy>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/aipo#acquiredInCourse -->

    <owl:ObjectProperty rdf:about="https://w3id.org/aipo#acquiredInCourse">
        <owl:inverseOf rdf:resource="https://w3id.org/aipo#aimsToLo"/>
        <rdfs:domain rdf:resource="https://w3id.org/aipo#LearningOutcome"/>
        <rdfs:range rdf:resource="https://w3id.org/aipo#Syllabus"/>
        <rdfs:comment xml:lang="en">A learning outcome  is acquired in a course</rdfs:comment>
        <rdfs:comment xml:lang="es">Un resultado de aprendizaje es adquirido en un curso</rdfs:comment>
        <rdfs:label xml:lang="en">acquiredInCourse</rdfs:label>
        <rdfs:label xml:lang="es">adquirido en una asignatura</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/aipo#affiliatedWith -->

    <owl:ObjectProperty rdf:about="https://w3id.org/aipo#affiliatedWith">
        <rdfs:comment xml:lang="en">Superproperty of some object properties that represents any type of affiliation relationship between the following classes: Educational Organization, Person, Curriculum, Course</rdfs:comment>
        <rdfs:comment xml:lang="es">Superpropiedad de algunas propiedades de objeto que representa cualquier tipo de relación de afiliación entre las siguientes clases: Organización Educativa, Persona, Programa de Estudio, Asignatura</rdfs:comment>
        <rdfs:label xml:lang="en">affiliated With</rdfs:label>
        <rdfs:label xml:lang="es">afiliado con</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/aipo#aimsToLo -->

    <owl:ObjectProperty rdf:about="https://w3id.org/aipo#aimsToLo">
        <rdfs:domain rdf:resource="https://w3id.org/aipo#Syllabus"/>
        <rdfs:range rdf:resource="https://w3id.org/aipo#LearningOutcome"/>
        <rdfs:comment xml:lang="en">A subject achieves a learning outcome</rdfs:comment>
        <rdfs:comment xml:lang="es">Una asignatura logra un resultado de aprendizaje</rdfs:comment>
        <rdfs:label xml:lang="en">aims to Learning Outcome</rdfs:label>
        <rdfs:label xml:lang="es">logra un resultado de aprendizaje</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/aipo#alumnusOf -->

    <owl:ObjectProperty rdf:about="https://w3id.org/aipo#alumnusOf">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/aipo#affiliatedWith"/>
        <rdfs:subPropertyOf>
            <rdf:Description>
                <owl:inverseOf rdf:resource="https://w3id.org/aipo#affiliatedWith"/>
            </rdf:Description>
        </rdfs:subPropertyOf>
        <owl:inverseOf rdf:resource="https://w3id.org/aipo#hasAlumnus"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/aipo#Person"/>
        <rdfs:range rdf:resource="https://w3id.org/aipo#Departament"/>
        <rdfs:comment xml:lang="en">A student is a student of a Professional School</rdfs:comment>
        <rdfs:comment xml:lang="es">Un estudiante es alumno una  Escuela Profesional</rdfs:comment>
        <rdfs:label xml:lang="es">alumno de</rdfs:label>
        <rdfs:label xml:lang="en">alumnus of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/aipo#attendsCourse -->

    <owl:ObjectProperty rdf:about="https://w3id.org/aipo#attendsCourse">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/aipo#affiliatedWith"/>
        <owl:inverseOf rdf:resource="https://w3id.org/aipo#hasAttender"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/aipo#Student"/>
        <rdfs:range rdf:resource="https://w3id.org/aipo#Course"/>
        <owl:propertyDisjointWith rdf:resource="https://w3id.org/aipo#hasCompleted"/>
        <owl:propertyDisjointWith rdf:resource="https://w3id.org/aipo#instructorOf"/>
        <rdfs:comment xml:lang="en">A student attends one or more subjects or courses</rdfs:comment>
        <rdfs:comment xml:lang="es">Un estudiante asiste a una o varias asignaturas o cursos</rdfs:comment>
        <rdfs:label xml:lang="es">asiste a una asignatura</rdfs:label>
        <rdfs:label xml:lang="en">attends course</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/aipo#belongsTo -->

    <owl:ObjectProperty rdf:about="https://w3id.org/aipo#belongsTo">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="http://schema.org/EducationalOrganization"/>
        <rdfs:range rdf:resource="http://schema.org/EducationalOrganization"/>
        <rdfs:comment xml:lang="en">A professional school belongs to a University</rdfs:comment>
        <rdfs:comment xml:lang="es">Una Escuela profesional pertenece a una Universidad</rdfs:comment>
        <rdfs:label xml:lang="en">belongs to</rdfs:label>
        <rdfs:label xml:lang="es">pertenece a</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/aipo#containsTo -->

    <owl:ObjectProperty rdf:about="https://w3id.org/aipo#containsTo">
        <owl:inverseOf rdf:resource="https://w3id.org/aipo#isLinked"/>
        <rdfs:domain rdf:resource="https://w3id.org/aipo#StrategicAxis"/>
        <rdfs:range rdf:resource="https://w3id.org/aipo#Course"/>
        <rdfs:comment xml:lang="en">A strategic axis contains one or more courses</rdfs:comment>
        <rdfs:comment xml:lang="es">Un eje estratégico contiene uno o varios cursos</rdfs:comment>
        <rdfs:label xml:lang="en">contains to</rdfs:label>
        <rdfs:label xml:lang="es">contiene a</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/aipo#contributesTo -->

    <owl:ObjectProperty rdf:about="https://w3id.org/aipo#contributesTo">
        <rdfs:domain rdf:resource="https://w3id.org/aipo#Course"/>
        <rdfs:range rdf:resource="https://w3id.org/aipo#Competences"/>
        <rdfs:comment xml:lang="en">One or more subjects contribute to one or more consentences</rdfs:comment>
        <rdfs:comment xml:lang="es">Una o varias asignaturas  contribuyen a una o varias compentencias</rdfs:comment>
        <rdfs:label xml:lang="en">contributes to</rdfs:label>
        <rdfs:label xml:lang="es">contribuye a</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/aipo#develops -->

    <owl:ObjectProperty rdf:about="https://w3id.org/aipo#develops">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/aipo#contributesTo"/>
        <rdfs:comment xml:lang="en">Competence with a TOTAL correspondence to the curriculum. The contribution of the Curriculum is greater than 70%</rdfs:comment>
        <rdfs:comment xml:lang="es">Competencia con una correspondencia TOTAL al plan de estudio. La contribución del Plan de Estudio es mayor al 70 %</rdfs:comment>
        <rdfs:label xml:lang="es">desarrolla</rdfs:label>
        <rdfs:label xml:lang="en">develops</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/aipo#enrolledIn -->

    <owl:ObjectProperty rdf:about="https://w3id.org/aipo#enrolledIn">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/aipo#affiliatedWith"/>
        <rdfs:subPropertyOf>
            <rdf:Description>
                <owl:inverseOf rdf:resource="https://w3id.org/aipo#affiliatedWith"/>
            </rdf:Description>
        </rdfs:subPropertyOf>
        <owl:inverseOf rdf:resource="https://w3id.org/aipo#hasRegistered"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/aipo#Student"/>
        <rdfs:range rdf:resource="https://w3id.org/aipo#ProgramOfStudy"/>
        <owl:propertyDisjointWith rdf:resource="https://w3id.org/aipo#hasDegree"/>
        <rdfs:comment xml:lang="en">A student is enrolled in a program of study</rdfs:comment>
        <rdfs:comment xml:lang="es">Un estudiante esta inscrito en un programa de estudios</rdfs:comment>
        <rdfs:label xml:lang="en">enrolled in</rdfs:label>
        <rdfs:label xml:lang="es">inscrito en</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/aipo#followsEM -->

    <owl:ObjectProperty rdf:about="https://w3id.org/aipo#followsEM">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/aipo#Syllabus"/>
        <rdfs:range rdf:resource="https://w3id.org/aipo#EvaluationMethod"/>
        <rdfs:comment xml:lang="es">Una asignatura tiene una o mas metodologías de evaluación</rdfs:comment>
        <rdfs:label xml:lang="en">follows evaluation method</rdfs:label>
        <rdfs:label xml:lang="es">tiene una metodología de evaluación</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/aipo#followsTM -->

    <owl:ObjectProperty rdf:about="https://w3id.org/aipo#followsTM">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/aipo#Syllabus"/>
        <rdfs:range rdf:resource="https://w3id.org/aipo#TeachingMethod"/>
        <rdfs:comment xml:lang="en">Course has teaching method</rdfs:comment>
        <rdfs:label xml:lang="es">Un curso tiene un metódo de enseñanza</rdfs:label>
        <rdfs:label xml:lang="en">follows teaching method</rdfs:label>
        <rdfs:label xml:lang="es">sigue metodología de enseñanza</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/aipo#hasAcademicStaff -->

    <owl:ObjectProperty rdf:about="https://w3id.org/aipo#hasAcademicStaff">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/aipo#affiliatedWith"/>
        <owl:inverseOf rdf:resource="https://w3id.org/aipo#memberOf"/>
        <rdfs:domain rdf:resource="https://w3id.org/aipo#Departament"/>
        <rdfs:range rdf:resource="https://w3id.org/aipo#AcademicStaff"/>
        <rdfs:comment xml:lang="es">La escuela profesional tiene uno varios personal académico</rdfs:comment>
        <rdfs:comment xml:lang="en">The professional school has one of several academic staff</rdfs:comment>
        <rdfs:label xml:lang="en">has academic staff</rdfs:label>
        <rdfs:label xml:lang="es">tiene personal académico</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/aipo#hasAlumnus -->

    <owl:ObjectProperty rdf:about="https://w3id.org/aipo#hasAlumnus">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/aipo#affiliatedWith"/>
        <rdfs:subPropertyOf>
            <rdf:Description>
                <owl:inverseOf rdf:resource="https://w3id.org/aipo#affiliatedWith"/>
            </rdf:Description>
        </rdfs:subPropertyOf>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/aipo#Departament"/>
        <rdfs:range rdf:resource="https://w3id.org/aipo#Person"/>
        <rdfs:comment xml:lang="en">A professional school has one or more students</rdfs:comment>
        <rdfs:comment xml:lang="es">Una escuela profesional tiene uno o varios alumnos</rdfs:comment>
        <rdfs:label xml:lang="en">has alumnus</rdfs:label>
        <rdfs:label xml:lang="es">tiene alumnos</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/aipo#hasAssitant -->

    <owl:ObjectProperty rdf:about="https://w3id.org/aipo#hasAssitant">
        <rdfs:domain rdf:resource="https://w3id.org/aipo#Syllabus"/>
        <rdfs:range rdf:resource="https://w3id.org/aipo#Professor"/>
        <owl:propertyDisjointWith rdf:resource="https://w3id.org/aipo#hasAttender"/>
        <owl:propertyDisjointWith rdf:resource="https://w3id.org/aipo#hasInstructor"/>
        <owl:propertyDisjointWith rdf:resource="https://w3id.org/aipo#hasSupervisor"/>
        <rdfs:comment xml:lang="es">Una asignatura tiene docente adjunto</rdfs:comment>
        <rdfs:label xml:lang="en">A course has an assitant</rdfs:label>
        <rdfs:label xml:lang="en">has Assistant</rdfs:label>
        <rdfs:label xml:lang="es">tiene docente adjunto</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/aipo#hasAttender -->

    <owl:ObjectProperty rdf:about="https://w3id.org/aipo#hasAttender">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/aipo#affiliatedWith"/>
        <rdfs:subPropertyOf>
            <rdf:Description>
                <owl:inverseOf rdf:resource="https://w3id.org/aipo#affiliatedWith"/>
            </rdf:Description>
        </rdfs:subPropertyOf>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/aipo#Course"/>
        <rdfs:range rdf:resource="https://w3id.org/aipo#Student"/>
        <owl:propertyDisjointWith rdf:resource="https://w3id.org/aipo#hasCreator"/>
        <owl:propertyDisjointWith rdf:resource="https://w3id.org/aipo#hasInstructor"/>
        <owl:propertyDisjointWith rdf:resource="https://w3id.org/aipo#hasSupervisor"/>
        <rdfs:comment xml:lang="en">A course has one or more attendees</rdfs:comment>
        <rdfs:comment xml:lang="es">Un curso tiene uno o varios asistentes</rdfs:comment>
        <rdfs:label xml:lang="en">has attender</rdfs:label>
        <rdfs:label xml:lang="es">tiene asistentes</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/aipo#hasCompetencesLA -->

    <owl:ObjectProperty rdf:about="https://w3id.org/aipo#hasCompetencesLA">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/aipo#isJoinned"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/aipo#IntegralFormation"/>
        <rdfs:range rdf:resource="https://w3id.org/aipo#IntegralFormation"/>
        <rdfs:comment xml:lang="es">El eje  estratégico de  enseñanza aprendizaje tiene uno o varias competencias</rdfs:comment>
        <rdfs:comment xml:lang="en">The strategic axis of learning education has one or more competencies</rdfs:comment>
        <rdfs:label xml:lang="en">has competences</rdfs:label>
        <rdfs:label xml:lang="es">tiene competencias</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/aipo#hasCompleted -->

    <owl:ObjectProperty rdf:about="https://w3id.org/aipo#hasCompleted">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/aipo#affiliatedWith"/>
        <rdfs:domain rdf:resource="https://w3id.org/aipo#Student"/>
        <rdfs:range rdf:resource="https://w3id.org/aipo#Course"/>
        <rdfs:comment xml:lang="en">A student completed one or more courses</rdfs:comment>
        <rdfs:comment xml:lang="es">Un estudiante completó uno o varios cursos</rdfs:comment>
        <rdfs:label xml:lang="es">ha completado</rdfs:label>
        <rdfs:label xml:lang="en">has completed</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/aipo#hasCreator -->

    <owl:ObjectProperty rdf:about="https://w3id.org/aipo#hasCreator">
        <rdfs:domain rdf:resource="https://w3id.org/aipo#Syllabus"/>
        <rdfs:range rdf:resource="https://w3id.org/aipo#AdministrativeStaff"/>
        <rdfs:comment xml:lang="es">El sílabo tiene una persona quien lo elabora</rdfs:comment>
        <rdfs:comment xml:lang="en">The syllabus has a person who makes it</rdfs:comment>
        <rdfs:label xml:lang="en">has creator</rdfs:label>
        <rdfs:label xml:lang="es">tiene creador</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/aipo#hasDegree -->

    <owl:ObjectProperty rdf:about="https://w3id.org/aipo#hasDegree">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/aipo#affiliatedWith"/>
        <rdfs:domain rdf:resource="https://w3id.org/aipo#Person"/>
        <rdfs:range rdf:resource="https://w3id.org/aipo#ProgramOfStudy"/>
        <rdfs:comment xml:lang="en">A person has one or more academic degrees</rdfs:comment>
        <rdfs:comment xml:lang="es">Una persona tiene uno o varios grados académicos</rdfs:comment>
        <rdfs:label xml:lang="en">has degree</rdfs:label>
        <rdfs:label xml:lang="es">tiene grado</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/aipo#hasEmployee -->

    <owl:ObjectProperty rdf:about="https://w3id.org/aipo#hasEmployee">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/aipo#affiliatedWith"/>
        <owl:inverseOf rdf:resource="https://w3id.org/aipo#worksFor"/>
        <rdfs:domain rdf:resource="http://schema.org/EducationalOrganization"/>
        <rdfs:range rdf:resource="https://w3id.org/aipo#Employee"/>
        <rdfs:comment xml:lang="en">An organization has one or more workers</rdfs:comment>
        <rdfs:comment xml:lang="es">Una organización tiene uno o varios trabajadores</rdfs:comment>
        <rdfs:label xml:lang="en">has employee</rdfs:label>
        <rdfs:label xml:lang="es">tiene empleado</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/aipo#hasEvent -->

    <owl:ObjectProperty rdf:about="https://w3id.org/aipo#hasEvent">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/aipo#isJoinned"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/aipo#StrategicAxis"/>
        <rdfs:range rdf:resource="https://w3id.org/aipo#StrategicAxis"/>
        <rdfs:comment xml:lang="en">A strategic axis has one or more events that are developed by the faculties and those responsible for the axis</rdfs:comment>
        <rdfs:comment xml:lang="es">Un eje estratégico  tiene uno o varios eventos que son desarrollados por los responsables del eje</rdfs:comment>
        <rdfs:label xml:lang="en">has event</rdfs:label>
        <rdfs:label xml:lang="es">tiene evento</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/aipo#hasInstructor -->

    <owl:ObjectProperty rdf:about="https://w3id.org/aipo#hasInstructor">
        <rdfs:domain>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://w3id.org/aipo#Course"/>
                    <rdf:Description rdf:about="https://w3id.org/aipo#Syllabus"/>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="https://w3id.org/aipo#Professor"/>
        <rdfs:comment xml:lang="en">A course has an instructor who is a teacher</rdfs:comment>
        <rdfs:comment xml:lang="es">Una asignatura tiene un instructor que es un docente</rdfs:comment>
        <rdfs:label xml:lang="en">has instructor</rdfs:label>
        <rdfs:label xml:lang="es">tiene instructor</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/aipo#hasLearningOutcome -->

    <owl:ObjectProperty rdf:about="https://w3id.org/aipo#hasLearningOutcome">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/aipo#Course"/>
        <rdfs:range rdf:resource="https://w3id.org/aipo#LearningOutcome"/>
        <rdfs:comment xml:lang="en">A course has one or more learning outcomes</rdfs:comment>
