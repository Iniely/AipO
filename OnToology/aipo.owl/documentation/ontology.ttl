     rdfs:subPropertyOf owl:topDataProperty ;
     rdfs:domain <http://schema.org/EducationalOrganization> ;
     rdfs:range xsd:anyURI ;
     rdfs:label "dirección web de la Universidad"@es ,
                "university's web address"@en .


###  https://w3id.org/aipo/aipo#workingCondition
:workingCondition rdf:type owl:DatatypeProperty ;
                  rdfs:domain :Employee ;
                  rdfs:range [ rdf:type rdfs:Datatype ;
                               owl:oneOf [ rdf:type rdf:List ;
                                           rdf:first "Employee" ;
                                           rdf:rest [ rdf:type rdf:List ;
                                                      rdf:first "Hired Full Time" ;
                                                      rdf:rest [ rdf:type rdf:List ;
                                                                 rdf:first "Hired Part Time" ;
                                                                 rdf:rest [ rdf:type rdf:List ;
                                                                            rdf:first "Missionary" ;
                                                                            rdf:rest rdf:nil
                                                                          ]
                                                               ]
                                                    ]
                                         ]
                             ] ;
                  rdfs:comment "A worker's employment status may be: Missionary, Part-time Contract, full-time contract, or employee"@en ,
                               "La condición laboral de un trabajador puede ser: Misionero, Contrado a tiempo parcial, contratado a tiempo completo o empleado"@es ;
                  rdfs:label "condición laboral"@es ,
                             "working condition"@en .


###  https://w3id.org/aipo/aipo#foaf:gender
:foaf:gender rdf:type owl:DatatypeProperty ,
                      owl:FunctionalProperty ;
             rdfs:domain :Person ;
             rdfs:range xsd:string ;
             rdfs:isDefinedBy <http://xmlns.com/foaf/0.1/> ;
             rdfs:label "gender"@en ,
                        "género"@es .


#################################################################
#    Classes
#################################################################

###  http://schema.org/EducationalOrganization
<http://schema.org/EducationalOrganization> rdf:type owl:Class ;
                                            rdfs:subClassOf <http://schema.org/Organization> ;
                                            rdfs:comment "Institución encargada de impartir servicios educativos autorizada por el estado para brindar programas de estudios que condiucen a un grado y título profesional."@es ,
                                                         "Institution in charge of providing educational services authorized by the state to provide study programs leading to a degree and professional title."@en ;
                                            rdfs:isDefinedBy <http://schema.org/EducationalOrganization> ;
                                            rdfs:label "Educational organization"@en ,
                                                       "Indtitución Educativa"@es .


###  http://schema.org/Organization
<http://schema.org/Organization> rdf:type owl:Class ;
                                 rdfs:comment "An organization is an association of people who interact with each other and use resources of various kinds in order to achieve certain objectives or goals."@es ,
                                              "Una organización es una asociación de personas que se relacionan entre sí y utilizan recursos de diversa índole con el fin de lograr determinados objetivos o metas."@es ;
                                 rdfs:label "Organización"@es ,
                                            "Organization"@en .


###  http://xmlns.com/foaf/0.1/Group
<http://xmlns.com/foaf/0.1/Group> rdf:type owl:Class ;
                                  rdfs:isDefinedBy "http://xmlns.com/foaf/0.1/" ;
                                  rdfs:label "Foaf Group"@en .


###  http://xmlns.com/foaf/0.1/Person
<http://xmlns.com/foaf/0.1/Person> rdf:type owl:Class ;
                                   owl:equivalentClass :Person ;
                                   rdfs:isDefinedBy "http://xmlns.com/foaf/0.1" .


###  https://w3id.org/aipo/aipo#AcademicStaff
:AcademicStaff rdf:type owl:Class ;
               rdfs:subClassOf :Employee ;
               owl:disjointWith :Student ,
                                :UndergraduateStudent ;
               rdfs:comment "Personal encargado del área académica de una Institución Educativa"@es ,
                            "Personnel in charge of the academic area of an Educational Institution"@en ;
               rdfs:label "Academic Staff"@en ,
                          "Personal Académico"@es .


###  https://w3id.org/aipo/aipo#AdministrativeStaff
:AdministrativeStaff rdf:type owl:Class ;
                     rdfs:subClassOf :Employee ;
                     owl:disjointWith :Student ,
                                      :UndergraduateStudent ;
                     rdfs:comment "Administrative staff of an organization"@en ,
                                  "Personal administrativo de una organizacion"@es ;
                     rdfs:label "Administrative Staff"@en ,
                                "Personal Administrativo"@es .


###  https://w3id.org/aipo/aipo#BachelorDegree
:BachelorDegree rdf:type owl:Class ;
                rdfs:subClassOf :ProgramOfStudy ;
                owl:disjointWith :Degree ;
                rdfs:comment "Academic degree awarded by a university to a person who has completed undergraduate studies and supported his research article."@en ,
                             "Grado académico otorgado por una universidad a una persona que ha completado los estudios de pregrado y sustenado su artículo de investigación."@es ;
                rdfs:label "Bachelor degree"@en ,
                           "Grado académico de bachiller"@es ;
                rdfs:seeAlso "https://drive.google.com/file/d/19MI3BAUe_493d8tolRqTAZZwt64NqtEe/view" .


###  https://w3id.org/aipo/aipo#CompetenceIF
:CompetenceIF rdf:type owl:Class ;
              rdfs:subClassOf :IntegralFormation ;
              rdfs:label "Competencias eje Enseñanza aprendizaje"@es .


###  https://w3id.org/aipo/aipo#Competences
:Competences rdf:type owl:Class ;
             rdfs:comment "A general statement detailing the desired knowledge and skills of the student graduating from the course or program of study."@en ,
                          "Una declaración general que detalla los conocimientos y habilidades deseados del estudiante que egresa del curso o programa de estudios."@es ;
             rdfs:label "Competences"@en ,
                        "Competencias"@es .


###  https://w3id.org/aipo/aipo#CoreCourse
:CoreCourse rdf:type owl:Class ;
            rdfs:subClassOf :Course ;
            owl:disjointWith :ElectiveCourse ;
            rdfs:comment "Courses that all students must complete before they can move to the next level / semester in their education."@en ,
                         "Cursos que todos los estudiantes deben completar antes de que puedan pasar al siguiente nivel/Ciclo en su educación."@es ;
            rdfs:label "Asignatura/Curso obligatorio"@es ,
                       "Core course"@en .


###  https://w3id.org/aipo/aipo#Course
:Course rdf:type owl:Class ;
        owl:equivalentClass [ rdf:type owl:Restriction ;
                              owl:onProperty :partOf ;
                              owl:someValuesFrom :KnowledgeArea
                            ] ;
        rdfs:comment "Cada una de las materias que forman parte de un plan de estudio"@es ,
                     "Each of the subjects that are part of a study plan"@en ;
        rdfs:label "Course"@en ,
                   "Curso/Asignatura"@es ;
        rdfs:seeAlso "Un curso puede ser: Génerico, Específico y de Especialidad" .


###  https://w3id.org/aipo/aipo#CulturalExtension
:CulturalExtension rdf:type owl:Class ;
                   owl:equivalentClass [ rdf:type owl:Restriction ;
                                         owl:onProperty :partOf ;
                                         owl:someValuesFrom :LinkedEnviroment
                                       ] ;
                   rdfs:subClassOf :StrategicAxis ;
                   rdfs:comment "Eje estratégico relacionado al servicio. Sus actividades son permanentes y se manifiestan mediante acciones de servicio a la comunidad o programas especiales de capacitación que la facultad o carrera profesional programe relacionadas a l logro de cometencias generales y específicas"@es ,
                                "Strategic axis related to service. Their activities are permanent and are manifested through community service actions or special training programs that the faculty or professional career programs related to the achievement of general and specific commitments."@en ;
                   rdfs:label "Cultural Extension"@en ,
                              "Extensión Cultural"@es .


###  https://w3id.org/aipo/aipo#Curriculum
:Curriculum rdf:type owl:Class ;
            rdfs:comment "Documento marco que contiene objetivos educacionales, perfil de egreso, resultados de aprendizaje, plan de estudios, sistema de evauación"@es ,
                         "Framework document containing educational objectives, graduation profile, learning outcomes, curriculum, assessment system"@en ;
            rdfs:label "Curriculum"@en ,
                       "Plan curricular"@es .


###  https://w3id.org/aipo/aipo#Degree
:Degree rdf:type owl:Class ;
        rdfs:subClassOf :ProgramOfStudy ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :requiresProgram ;
                          owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                          owl:onClass :BachelorDegree
                        ] ;
        rdfs:comment "To obtain the Degree, it is required to have a bachelor's degree and the support of a thesis or work of professional sufficiency"@en ,
                     "Para su obtención requiere poseer el grado de bachiller y la sustentación de una tesis o trabajo de suficiencia profesional" ;
        rdfs:label "Título profesional"@es .


###  https://w3id.org/aipo/aipo#Departament
:Departament rdf:type owl:Class ;
             rdfs:subClassOf <http://schema.org/EducationalOrganization> ;
             owl:disjointWith :Person ,
                              :School ,
                              :University ;
             rdfs:comment "Conjunto de estudios, generalmente universitarios, que habilitan para el ejercicio de una profesión"@es ,
                          "Group of studies, generally university, that qualify for the exercise of a profession"@en ;
             rdfs:label "Carrera Profesional/Escuela Profesional"@es ,
                        "Departament"@en .


###  https://w3id.org/aipo/aipo#EducationalProcess
:EducationalProcess rdf:type owl:Class ;
                    rdfs:subClassOf :IntegralFormation ;
                    rdfs:label "Proceso educativo"@es .


###  https://w3id.org/aipo/aipo#ElectiveCourse
:ElectiveCourse rdf:type owl:Class ;
                rdfs:subClassOf :Course ;
                rdfs:comment "Course that the student chooses voluntarily in order to meet a particular requirement and orient their training to some specialty"@en ,
                             "Curso que el estudiante elige por voluntad a fin de cumplir con un requisito en particular y orientar su formación a algunas especialidad"@es ;
                rdfs:label "Asignatura/Curso electivo"@es ,
                           "Elective course"@en .


###  https://w3id.org/aipo/aipo#Employee
:Employee rdf:type owl:Class ;
          owl:equivalentClass [ owl:intersectionOf ( :Person
                                                     [ rdf:type owl:Restriction ;
                                                       owl:onProperty :worksFor ;
                                                       owl:someValuesFrom <http://schema.org/EducationalOrganization>
                                                     ]
                                                   ) ;
                                rdf:type owl:Class
                              ] ;
          rdfs:subClassOf :Person ;
          rdfs:comment "Person who works in an institution"@en ,
                       "Persona que labora en una institución"@es ;
          rdfs:label "Empleado/Trabajdor"@es ,
                     "Employee"@en .


###  https://w3id.org/aipo/aipo#EvaluationMethod
:EvaluationMethod rdf:type owl:Class ;
                  owl:disjointWith :TeachingMethod ;
                  rdfs:comment "Methodology used by the teacher to measure the student's knowledge"@en ,
                               "Metodología utilizada por el docente para medir los conocimientos del estudiante"@es ;
                  rdfs:label "Evaluation Method"@en ,
                             "Metodología de evaluación"@es .


###  https://w3id.org/aipo/aipo#Generic
:Generic rdf:type owl:Class ;
         rdfs:subClassOf :Competences ;
         rdfs:comment "General competences are knowledge developed common to students of any career. These generic competences can be: instrumental, interpersonal and intrapersonal."@en ,
                      "Las competencias generales son saberes desarrollados común a estudiantes de cualquier carrera. Éstas competencias genéricas puesden ser: instrumentales, interpersonales e intrapersonales."@es ;
         rdfs:label "Generic"@en ,
                    "Genérico"@es .


###  https://w3id.org/aipo/aipo#GenericArea
:GenericArea rdf:type owl:Class ;
             rdfs:subClassOf :KnowledgeArea ;
             rdfs:comment "Orientada a capacitar a los estudiantes en los dominios de las capacidades y conocimientos básicos que son el soporte de aprendizajes más complejos y especializados."@es ,
                          "Oriented to train students in the domains of basic skills and knowledge that support more complex and specialized learning."@en ;
             rdfs:label "Generic Area"@en ,
                        "Área genérica"@es .


###  https://w3id.org/aipo/aipo#GraduationProfile
:GraduationProfile rdf:type owl:Class ;
                   rdfs:comment "El perfil de egreso se define como el conjunto de competencias (capacidades, habilidades, conocimientos, y actitudes) permiten que la persiba sea acreditada legalmente por una institución educativa, a fin de que pueda desempeñarse en su carrera y ser reconocido por la sociedad como un profesional."@es ,
                                "The graduate profile is defined as the set of competencies (capabilities, skills, knowledge, and attitudes) that allow the person to be legally accredited by an educational institution, so that he can perform in his career and be recognized by society as a professional."@en ;
                   rdfs:label "Graduation Profile"@en ,
                              "Perfil de egreso"@es .


###  https://w3id.org/aipo/aipo#IntegralFormation
:IntegralFormation rdf:type owl:Class ;
                   rdfs:subClassOf :StrategicAxis ;
                   rdfs:comment "Eje estratégico que hace referencia al proceso de enseñanza y aprendizaje de las carreras profesionales a través de actividades de diseño y evaluación curricular, movilidad estudiantil, nivelación, reforzamienro y prácticas pre prorfesioanles."@es ,
                                "Strategic axis that refers to the teaching and learning process of professional careers through curricular design and evaluation activities, student mobility, leveling, reinforcement and pre-professional practices."@en ;
                   rdfs:label "Formación Integral"@es ,
                              "Integral Formation"@en .


###  https://w3id.org/aipo/aipo#KnowledgeArea
:KnowledgeArea rdf:type owl:Class ;
               rdfs:comment "Orientada al desarrollo de las competencias de especialidad de una carrera. Cada área incluye las asignaturas que permiten lograr las competencias profesionales."@es ,
                            "Oriented to the development of the specialty competences of a career. Each area includes the subjects that allow the achievement of professional competencies."@en ;
               rdfs:label "Knowledge Area"@en ,
                          "Áreas de especialidad"@es .


###  https://w3id.org/aipo/aipo#LearningOutcome
:LearningOutcome rdf:type owl:Class ;
                 owl:equivalentClass [ rdf:type owl:Restriction ;
                                       owl:onProperty :partOf ;
                                       owl:someValuesFrom :Competences
                                     ] ;
                 rdfs:comment "Learning outcomes are statements of what the student is expected to be able to do, understand and / or be able to demonstrate once a learning process is completed. These results can be subject and for each syllable learning unit"@en ,
                              "Los resultados de aprendizaje son enunciados  de lo que se espera que el estudiante sea capaz de hacer, comprender y / o sea capaz de demostrar una vez terminado un proceso de aprendizaje. Estos resultados pueden ser de asignatura y por cada unidad de aprendizaje del sílabo"@es ;
                 rdfs:label "Learning Outcome"@en ,
                            "Resultado de aprendizaje"@es .


###  https://w3id.org/aipo/aipo#LinkedEnviroment
:LinkedEnviroment rdf:type owl:Class ;
                  rdfs:subClassOf :StrategicAxis ;
                  rdfs:comment "Eje estrategico que contiene los ejes extensión cultural y proyección social"@es ,
                               "Strategic axis that contains the cultural extension and social projection axes"@en ;
                  rdfs:label "Linked with the enviroment"@en ,
                             "Vinculación con el medio"@es .


###  https://w3id.org/aipo/aipo#Nationality
:Nationality rdf:type owl:Class ;
             rdfs:subClassOf <http://xmlns.com/foaf/0.1/Group> ;
             rdfs:comment "Condición que reconoce que una persona pertenece a un estado o nación."@es ,
                          "Condition that recognizes that a person belongs to a state or nation."@en ;
             rdfs:label "Nacionalidad"@es ,
                        "Nacionality"@en .


###  https://w3id.org/aipo/aipo#Person
:Person rdf:type owl:Class ;
        owl:disjointWith :School ,
                         :University ;
        rdfs:comment "A human being"@en ,
                     "Un ser humano"@es ;
        rdfs:label "Person"@en ,
                   "Persona"@es .


###  https://w3id.org/aipo/aipo#Professor
:Professor rdf:type owl:Class ;
           rdfs:subClassOf :AcademicStaff ;
           owl:disjointWith :UndergraduateStudent ;
           rdfs:comment "Profesional encargado de impartir una asignatura. Su papel es fundamental. El profesor debería ser un cristiano comprometido con el desarrollo espiritual, intelectual, físico y social del estudiante."@es ;
           rdfs:label "Docente"@es ,
                      "Professor"@en .


###  https://w3id.org/aipo/aipo#ProgramOfStudy
:ProgramOfStudy rdf:type owl:Class ;
                rdfs:comment "Es la oferta educativa que después de un proceso de diez ciclos académicos aprobados otorga, a nombre de la nación, un grado académico y un título porfesional respecto a una especialidad requerida en el mercado laboral"@es ,
                             "It is the educational offer that after a process of ten approved academic cycles grants, on behalf of the nation, an academic degree and a professional title regarding a specialty required in the labor market"@en ;
                rdfs:label "Program of study"@en ,
                           "Programa de estudios"@es .


###  https://w3id.org/aipo/aipo#Research
:Research rdf:type owl:Class ;
          rdfs:subClassOf :StrategicAxis ;
          rdfs:comment "El eje de investigación formativa, inherente al trabajo académico; desarrolla capacidades para elaborar y publicar trabajos de investigación que coducen la obtención de grados y títulos profesionales, monitoreadosn por la Dirección de Investigaciones de la Facultad."@es ,
                       "The formative research axis, inherent to academic work; develops capacities to elaborate and publish research works that lead to the obtaining of degrees and professional titles, monitored by the Research Directorate of the Faculty."@en ;
          rdfs:label "Investigación"@es ,
                     "Research"@en .


###  https://w3id.org/aipo/aipo#Researcher
:Researcher rdf:type owl:Class ;
            rdfs:subClassOf :AcademicStaff ;
            owl:disjointWith :UndergraduateStudent ;
            rdfs:comment "Docente encargado de realizar publicaciones científicas en la institución."@es ;
            rdfs:label "Investigador"@es ,
                       "Researcher"@en .


###  https://w3id.org/aipo/aipo#Resource
:Resource rdf:type owl:Class ;
          rdfs:label "Learning Resourse"@en ,
                     "Recursos de aprendizaje (material bibliográfico)"@es .


###  https://w3id.org/aipo/aipo#School
:School rdf:type owl:Class ;
        rdfs:subClassOf <http://schema.org/EducationalOrganization> ;
        owl:disjointWith :University ;
        rdfs:comment "Es un estamento que ofrece programas académicos respecto a una especialidad mediante una escuela académica"@es ,
                     "It is an establishment that offers academic programs regarding a specialty through an academic school"@en ;
        rdfs:label "Facultad"@es ,
                   "School"@en .


###  https://w3id.org/aipo/aipo#Semester
:Semester rdf:type owl:Class ;
          rdfs:comment "17-week academic period in Peru."@en ,
                       "Periodo académico de 17 semanas de duración, en el Perú."@es ;
          rdfs:label "Ciclo"@es ,
                     "Semester"@en .


###  https://w3id.org/aipo/aipo#SocialProjection
:SocialProjection rdf:type owl:Class ;
                  owl:equivalentClass [ rdf:type owl:Restriction ;
                                        owl:onProperty :partOf ;
                                        owl:someValuesFrom :LinkedEnviroment
                                      ] ;
                  rdfs:subClassOf :StrategicAxis ;
                  rdfs:comment "Estratégico relacionado al servicio mediante  actividades permanentes dirigidos a la comunidad y programados por  la facultad o carrera profesional."@es ,
                               "Strategic related to the service through permanent activities directed to the community and programmed by the faculty or professional career."@en ;
                  rdfs:label "Proyección Social"@es ,
                             "Social Projection"@en .


###  https://w3id.org/aipo/aipo#SpecialtyArea
:SpecialtyArea rdf:type owl:Class ;
               rdfs:subClassOf :KnowledgeArea ;
               rdfs:comment "Orientada al desarrollo de competencias específicas relacionadas con una disciplina o carrera profesional. En este caso se trata del logro de saberes referidos a la carrera de Administración y Negocios Internacionales y sus disciplinas esenciales"@es ,
                            "Oriented to the development of specific competences related to a discipline or professional career. In this case, it is about the achievement of knowledge related to the career of Administration and International Business and its essential disciplines"@en ;
               rdfs:label "Specialty Area"@en ,
                          "Área específica"@es .


###  https://w3id.org/aipo/aipo#Specific
:Specific rdf:type owl:Class ;
          rdfs:subClassOf :Competences ;
          rdfs:comment "Las competencias específicas son aquellos saberes que el estudiante adquiere y/o desarrolla en función a la carrera que eligieron. Las competencias específicas están relacionadas con las áreas de especialidad o funciones clave de la carrera profesional."@es ,
                       "The specific competences are those knowledge that the student acquires and / or develops according to the career they chose. Specific competencies are related to specialty areas or key career functions."@en ;
          rdfs:label "Específico"@es ,
                     "Specific"@en .


###  https://w3id.org/aipo/aipo#StrategicAxis
:StrategicAxis rdf:type owl:Class ;
               rdfs:comment "Los ejes estratégicos, son las líneas o rutas básicas que tiene una institución para el desarrollo de su plan académico. Estos ejes proporcionan orden y claridad mediante planes específicos y medibles."@es ,
                            "The strategic axes are the basic lines or routes that an institution has for the development of its academic plan. These axes provide order and clarity through specific and measurable plans."@en ;
               rdfs:label "Eje estratégico"@es ,
                          "Strategic Axis"@en .


###  https://w3id.org/aipo/aipo#Student
:Student rdf:type owl:Class ;
         rdfs:subClassOf :Person ;
         rdfs:comment "A person formally committed to learning, enrolled. As a child of God, the college student is the object of educational endeavor as a whole and, as such, should be loved and accepted"@en ,
                      "Una persona comprometida formalmente con el aprendizaje, matriculado. Como hijo de Dios, el estudiante universitario es el objeto del esfuerzo educativo como un todo y, como tal, debería ser amado y aceptado"@es ;
         rdfs:label "Estudiante"@es ,
                    "Student"@en .


###  https://w3id.org/aipo/aipo#SummerSemester
:SummerSemester rdf:type owl:Class ;
                rdfs:subClassOf :Semester ;
                rdfs:comment "8-week academic period, from January to March. This cycle is generally aimed at new entrants to take basic courses such as: Mathematics and Language"@en ,
                             "Periodo académico de 8 semanas, de enero a marzo. Este ciclo generalmente esta dirigido a los nuevos ingresantes para llevar cursos básicos como: Matemáticas y Lenguaje"@es ;
                rdfs:label "Ciclo de verano /Ciclo Cero"@es ,
                           "Summer Semester"@en .


###  https://w3id.org/aipo/aipo#Syllabus
:Syllabus rdf:type owl:Class ;
          rdfs:comment "Document that contains information on a subject through which the learning results and evidences that decalculate in the competencies of the professional profile are achieved"@en ,
                       "Documento que contiene información de una asignatura mediante la cual se logra los resultados y evidencias de aprendizaje que decalran en las competencias del perfil profesional. El sílabo de un curso genérico es elaborado por su respectivo departamento académico, si es un un curso de especialidad o especifico, éstees elaborado por la Escuela Académica."@es ;
          rdfs:label "Syllabus"@en ,
                     "Sílabo"@es .


###  https://w3id.org/aipo/aipo#TeachingMethod
:TeachingMethod rdf:type owl:Class ;
                rdfs:comment "Conjunto de técnicas y actividades que un profesor utiliza con el fin de lograr uno o varios objetivos educativos."@es ,
                             "Set of techniques and activities that a teacher uses in order to achieve one or more educational objectives."@en ;
                rdfs:label "Metódo de enseñanza"@es ,
                           "Teaching Method"@en .


###  https://w3id.org/aipo/aipo#TechnicalStaff
:TechnicalStaff rdf:type owl:Class ;
                rdfs:subClassOf :Employee ;
                rdfs:comment "Personal encargado del área tecnología de la Escuela de Administración"@es ,
                             "Personnel in charge of the technology area of the School of Administration"@en ;
                rdfs:label "Personal técnico"@es ,
                           "Technical Staff"@en .


###  https://w3id.org/aipo/aipo#Term
:Term rdf:type owl:Class ;
      rdfs:comment "Period that contains semesters, which can be developed in sixteen (16) weeks"@en ,
                   "Periodo que contiene semestres, que pueden ser desarrollado en dieciseis (16) semanas"@es ;
      rdfs:label "Año de estudio"@es ,
                 "Term"@en .


###  https://w3id.org/aipo/aipo#Topic
:Topic rdf:type owl:Class ;
       rdfs:comment "Contenido teórico práctico  específico de una determinada asignatura."@es ,
                    "Theoretical and practical content, specific to a certain subject."@en ;
       rdfs:label "Tema"@es ,
                  "Topic"@en .


###  https://w3id.org/aipo/aipo#UndergraduateStudent
:UndergraduateStudent rdf:type owl:Class ;
                      rdfs:subClassOf [ owl:intersectionOf ( :Student
                                                             [ rdf:type owl:Restriction ;
                                                               owl:onProperty :enrolledIn ;
