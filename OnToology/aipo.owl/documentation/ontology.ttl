#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/elements/1.1/rights
<http://purl.org/dc/elements/1.1/rights> rdf:type owl:AnnotationProperty .


#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2001/XMLSchema#duration
xsd:duration rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  http://purl.org/dc/elements/1.1/creator
<http://purl.org/dc/elements/1.1/creator> rdf:type owl:ObjectProperty ,
                                                   owl:AsymmetricProperty ,
                                                   owl:IrreflexiveProperty ;
                                          rdfs:domain :Resource ;
                                          rdfs:range :Person ;
                                          rdfs:comment "Responsable de elaborar el recurso. Puede ser una entidad, una persona o un servicio."@es .


###  http://xmlns.com/foaf/0.1/member
<http://xmlns.com/foaf/0.1/member> rdf:type owl:ObjectProperty ;
                                   rdfs:isDefinedBy "http://xmlns.com/foaf/0.1/" .


###  https://w3id.org/aipo/aipo#acquiredInCourse
:acquiredInCourse rdf:type owl:ObjectProperty ;
                  owl:inverseOf :aimsToLo ;
                  rdfs:domain :LearningOutcome ;
                  rdfs:range :Syllabus ;
                  rdfs:comment "A learning outcome  is acquired in a course"@en ,
                               "Un resultado de aprendizaje es adquirido en un curso"@es ;
                  rdfs:label "acquiredInCourse"@en ,
                             "adquirido en una asignatura"@es .


###  https://w3id.org/aipo/aipo#affiliatedWith
:affiliatedWith rdf:type owl:ObjectProperty ;
                rdfs:comment "Superproperty of some object properties that represents any type of affiliation relationship between the following classes: Educational Organization, Person, Curriculum, Course"@en ,
                             "Superpropiedad de algunas propiedades de objeto que representa cualquier tipo de relación de afiliación entre las siguientes clases: Organización Educativa, Persona, Programa de Estudio, Asignatura"@es ;
                rdfs:label "affiliated With"@en ,
                           "afiliado con"@es .


###  https://w3id.org/aipo/aipo#aimsToLo
:aimsToLo rdf:type owl:ObjectProperty ;
          rdfs:domain :Syllabus ;
          rdfs:range :LearningOutcome ;
          rdfs:comment "A subject achieves a learning outcome"@en ,
                       "Una asignatura logra un resultado de aprendizaje"@es ;
          rdfs:label "aims to Learning Outcome"@en ,
                     "logra resultado de aprendizaje"@es .


###  https://w3id.org/aipo/aipo#alumnusOf
:alumnusOf rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf :affiliatedWith ,
                              [ owl:inverseOf :affiliatedWith
                              ] ;
           owl:inverseOf :hasAlumnus ;
           rdf:type owl:AsymmetricProperty ,
                    owl:IrreflexiveProperty ;
           rdfs:domain :Person ;
           rdfs:range :Departament ;
           rdfs:comment "A student is a student of a Professional School"@en ,
                        "Un estudiante es alumno una  Escuela Profesional"@es ;
           rdfs:label "alumno de"@es ,
                      "alumnus of"@en .


###  https://w3id.org/aipo/aipo#attendsCourse
:attendsCourse rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf :affiliatedWith ;
               owl:inverseOf :hasAttender ;
               rdf:type owl:AsymmetricProperty ,
                        owl:IrreflexiveProperty ;
               rdfs:domain :Student ;
               rdfs:range :Course ;
               owl:propertyDisjointWith :hasCompleted ,
                                        :instructorOf ;
               rdfs:comment "A student attends one or more subjects or courses"@en ,
                            "Un estudiante asiste a una o varias asignaturas o cursos"@es ;
               rdfs:label "asiste a una asignatura"@es ,
                          "attends course"@en .


###  https://w3id.org/aipo/aipo#belongsTo
:belongsTo rdf:type owl:ObjectProperty ,
                    owl:TransitiveProperty ;
           rdfs:domain <http://schema.org/EducationalOrganization> ;
           rdfs:range <http://schema.org/EducationalOrganization> ;
           rdfs:comment "A professional school belongs to a University"@en ,
                        "Una Escuela profesional pertenece a una Universidad"@es ;
           rdfs:label "belongs to"@en ,
                      "pertenece a"@es .


###  https://w3id.org/aipo/aipo#containsTo
:containsTo rdf:type owl:ObjectProperty ;
            owl:inverseOf :isLinked ;
            rdfs:domain :StrategicAxis ;
            rdfs:range :Course ;
            rdfs:comment "A strategic axis contains one or more courses"@en ,
                         "Un eje estratégico contiene uno o varios cursos"@es ;
            rdfs:label "contains to"@en ,
                       "contiene a"@es .


###  https://w3id.org/aipo/aipo#contributesTo
:contributesTo rdf:type owl:ObjectProperty ;
               rdfs:domain :Course ;
               rdfs:range :Competences ;
               rdfs:comment "One or more subjects contribute to one or more consentences"@en ,
                            "Una o varias asignaturas  contribuyen a una o varias compentencias"@es ;
               rdfs:label "contributes to"@en ,
                          "contribuye a"@es .


###  https://w3id.org/aipo/aipo#develops
:develops rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf :contributesTo ;
          rdfs:comment "Competence with a TOTAL correspondence to the curriculum. The contribution of the Curriculum is greater than 70%"@en ,
                       "Competencia con una correspondencia TOTAL al plan de estudio. La contribución del Plan de Estudio es mayor al 70 %"@es ;
          rdfs:label "desarrolla"@es ,
                     "develops"@en .


###  https://w3id.org/aipo/aipo#enrolledIn
:enrolledIn rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf :affiliatedWith ,
                               [ owl:inverseOf :affiliatedWith
                               ] ;
            owl:inverseOf :hasRegistered ;
            rdf:type owl:AsymmetricProperty ,
                     owl:IrreflexiveProperty ;
            rdfs:domain :Student ;
            rdfs:range :ProgramOfStudy ;
            owl:propertyDisjointWith :hasDegree ;
            rdfs:comment "A student is enrolled in a program of study"@en ,
                         "Un estudiante esta inscrito en un programa de estudios"@es ;
            rdfs:label "enrolled in"@en ,
                       "inscrito en"@es .


###  https://w3id.org/aipo/aipo#followsEM
:followsEM rdf:type owl:ObjectProperty ,
                    owl:AsymmetricProperty ,
                    owl:IrreflexiveProperty ;
           rdfs:domain :Syllabus ;
           rdfs:range :EvaluationMethod ;
           rdfs:comment "Una asignatura tiene una o mas metodologías de evaluación"@es ;
           rdfs:label "follows evaluation method"@en ,
                      "tiene metodología de evaluación"@es .


###  https://w3id.org/aipo/aipo#followsTM
:followsTM rdf:type owl:ObjectProperty ,
                    owl:AsymmetricProperty ,
                    owl:IrreflexiveProperty ;
           rdfs:domain :Syllabus ;
           rdfs:range :TeachingMethod ;
           rdfs:comment "Course has teaching method"@en ,
                        "Un curso tiene uno o varios metódos de enseñanza"@es ;
           rdfs:label "follows teaching method"@en ,
                      "sigue metodología de enseñanza"@es .


###  https://w3id.org/aipo/aipo#hasAcademicStaff
:hasAcademicStaff rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf :affiliatedWith ;
                  owl:inverseOf :memberOf ;
                  rdfs:domain :Departament ;
                  rdfs:range :AcademicStaff ;
                  rdfs:comment "La escuela profesional tiene uno varios personal académico"@es ,
                               "The professional school has one of several academic staff"@en ;
                  rdfs:label "has academic staff"@en ,
                             "tiene personal académico"@es .


###  https://w3id.org/aipo/aipo#hasAlumnus
:hasAlumnus rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf :affiliatedWith ,
                               [ owl:inverseOf :affiliatedWith
                               ] ;
            rdf:type owl:AsymmetricProperty ,
                     owl:IrreflexiveProperty ;
            rdfs:domain :Departament ;
            rdfs:range :Person ;
            rdfs:comment "A professional school has one or more students"@en ,
                         "Una escuela profesional tiene uno o varios alumnos"@es ;
            rdfs:label "has alumnus"@en ,
                       "tiene alumnos"@es .


###  https://w3id.org/aipo/aipo#hasAssitant
:hasAssitant rdf:type owl:ObjectProperty ;
             rdfs:domain :Syllabus ;
             rdfs:range :Professor ;
             owl:propertyDisjointWith :hasAttender ,
                                      :hasInstructor ,
                                      :hasSupervisor ;
             rdfs:comment "Una asignatura tiene docente adjunto"@es ;
             rdfs:label "a course has an assitant"@en ,
                        "has Assistant"@en ,
                        "tiene docente adjunto"@es .


###  https://w3id.org/aipo/aipo#hasAttender
:hasAttender rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf :affiliatedWith ,
                                [ owl:inverseOf :affiliatedWith
                                ] ;
             rdf:type owl:AsymmetricProperty ,
                      owl:IrreflexiveProperty ;
             rdfs:domain :Course ;
             rdfs:range :Student ;
             owl:propertyDisjointWith :hasCreator ,
                                      :hasInstructor ,
                                      :hasSupervisor ;
             rdfs:comment "A course has one or more attendees"@en ,
                          "Un curso tiene uno o varios asistentes"@es ;
             rdfs:label "has attender"@en ,
                        "tiene asistentes"@es .


###  https://w3id.org/aipo/aipo#hasCompleted
:hasCompleted rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf :affiliatedWith ;
              rdfs:domain :Student ;
              rdfs:range :Course ;
              rdfs:comment "A student completed one or more courses"@en ,
                           "Un estudiante completó uno o varios cursos"@es ;
              rdfs:label "ha completado"@es ,
                         "has completed"@en .


###  https://w3id.org/aipo/aipo#hasCreator
:hasCreator rdf:type owl:ObjectProperty ;
            rdfs:domain :Syllabus ;
            rdfs:range :AdministrativeStaff ;
            rdfs:comment "El sílabo tiene una persona quien lo elabora"@es ,
                         "The syllabus has a person who makes it"@en ;
            rdfs:label "has creator"@en ,
                       "tiene creador"@es .


###  https://w3id.org/aipo/aipo#hasDegree
:hasDegree rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf :affiliatedWith ;
           rdfs:domain :Person ;
           rdfs:range :ProgramOfStudy ;
           rdfs:comment "A person has one or more academic degrees"@en ,
                        "Una persona tiene uno o varios grados académicos"@es ;
           rdfs:label "has degree"@en ,
                      "tiene grado"@es .


###  https://w3id.org/aipo/aipo#hasEmployee
:hasEmployee rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf :affiliatedWith ;
             owl:inverseOf :worksFor ;
             rdfs:domain <http://schema.org/EducationalOrganization> ;
             rdfs:range :Employee ;
             rdfs:comment "An organization has one or more workers"@en ,
                          "Una organización tiene uno o varios trabajadores"@es ;
             rdfs:label "has employee"@en ,
                        "tiene empleado"@es .


###  https://w3id.org/aipo/aipo#hasEvent
:hasEvent rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf :isJoinned ;
          rdf:type owl:TransitiveProperty ;
          rdfs:domain :StrategicAxis ;
          rdfs:range :StrategicAxis ;
          rdfs:comment "A strategic axis has one or more events that are developed by the faculties and those responsible for the axis"@en ,
                       "Un eje estratégico  tiene uno o varios eventos que son desarrollados por los responsables del eje"@es ;
          rdfs:label "has event"@en ,
                     "tiene evento"@es .


###  https://w3id.org/aipo/aipo#hasInstructor
:hasInstructor rdf:type owl:ObjectProperty ;
               rdfs:domain [ owl:intersectionOf ( :Course
                                                  :Syllabus
                                                ) ;
                             rdf:type owl:Class
                           ] ;
               rdfs:range :Professor ;
               rdfs:comment "A course has an instructor who is a teacher"@en ,
                            "Una asignatura tiene un instructor que es un docente"@es ;
               rdfs:label "has instructor"@en ,
                          "tiene instructor"@es .


###  https://w3id.org/aipo/aipo#hasLearningOutcome
:hasLearningOutcome rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf owl:topObjectProperty ;
                    rdfs:domain :Course ;
                    rdfs:range :LearningOutcome ;
                    rdfs:comment "A course has one or more learning outcomes"@en ,
                                 "Una asignatura tiene uno o varios resultados de aprendizaje"@es ;
                    rdfs:label "has Learning Outcome"@en ,
                               "tiene resultado de aprendizaje"@es .


###  https://w3id.org/aipo/aipo#hasNationality
:hasNationality rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf <http://xmlns.com/foaf/0.1/member> ;
                rdfs:domain :Person ;
                rdfs:range :Nationality ;
                rdfs:comment "A person has one or more nationalities"@en ,
                             "Una persona tiene una o varias nacionalidades"@es ;
                rdfs:label "has nationality"@en ,
                           "tiene nacionalidad"@es .


###  https://w3id.org/aipo/aipo#hasPrerequisite
:hasPrerequisite rdf:type owl:ObjectProperty ,
                          owl:TransitiveProperty ;
                 rdfs:domain :Course ;
                 rdfs:range :Course ;
                 rdfs:comment "A course has one or more prerequisites"@en ,
                              "Una asignatura tiene uno o varios prerequisitos"@es ;
                 rdfs:label "has prerequisite"@en ,
                            "tiene prerequisito"@es .


###  https://w3id.org/aipo/aipo#hasProduct
:hasProduct rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf :isJoinned ;
            rdf:type owl:TransitiveProperty ;
